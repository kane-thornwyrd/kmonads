{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "recurseDepth": 10,
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "name": "Kmonads",
    "logo": {
      "url": "https://github.com/kane-thornwyrd/kmonads/blob/master/kmonads.png?raw=true",
      "width": "32px",
      "height": "32px",
      "link": "https://kane-thornwyrd.github.io/kmonads/"
    },
    "useCollapsibles": true,
    "footerText": "A Kane Thornwyrd Product"
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./docs/"
  }
}
